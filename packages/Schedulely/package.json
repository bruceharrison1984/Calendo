{
  "name": "schedulely",
  "version": "0.2.3",
  "keywords": [
    "schedule",
    "calendar",
    "react",
    "month"
  ],
  "sideEffects": [
    "*.css"
  ],
  "description": "CSS-grid based React calendar with a focus on extensibility and small package size",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "barrels": "barrelsby -c .barrelsby.json",
    "rollup": "rollup -c",
    "lint": "eslint . --ext .ts --ext .tsx",
    "prettier": "prettier --check './src/**.(ts|tsx|js|scss)'",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "npx jest --ci --json --coverage --testLocationInResults --outputFile=report.json --maxWorkers 1",
    "dev": "ladle serve --stories=__stories__/**.stories.tsx",
    "build-ladle": "ladle build"
  },
  "files": [
    "dist"
  ],
  "author": "Bruce Harrison",
  "license": "Apache-2.0",
  "homepage": "https://bruceharrison1984.github.io/Schedulely/",
  "repository": {
    "url": "https://github.com/bruceharrison1984/Schedulely",
    "type": "git"
  },
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "devDependencies": {
    "@ladle/react": "^2.15.0",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-inject": "^5.0.3",
    "@swc/core": "^1.3.68",
    "@swc/helpers": "^0.5.1",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/chance": "^1.1.3",
    "@types/node": "^18.11.4",
    "@types/react": "17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "autoprefixer": "^10.4.14",
    "barrelsby": "^2.8.0",
    "cssnano": "^6.0.1",
    "cssnano-preset-advanced": "^6.0.1",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.24",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "resize-observer-polyfill": "^1.5.1",
    "rollup": "^3.26.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-ts": "^3.2.0",
    "sass": "^1.63.6",
    "vitest": "^0.33.0"
  }
}
