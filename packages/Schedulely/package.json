{
  "name": "schedulely",
  "version": "0.2.2",
  "keywords": [
    "schedule",
    "calendar",
    "react",
    "month"
  ],
  "sideEffects": [
    "*.css"
  ],
  "description": "CSS-grid based React calendar with a focus on extensibility and small package size",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "barrels": "barrelsby -c .barrelsby.json",
    "rollup": "rollup -c",
    "lint": "eslint . --ext .ts --ext .tsx",
    "prettier": "prettier --check './src/**.(ts|tsx|js|scss)'",
    "test": "jest",
    "dev": "ladle serve --stories=__stories__/**.stories.tsx",
    "build-ladle": "ladle build"
  },
  "files": [
    "dist"
  ],
  "author": "Bruce Harrison",
  "license": "Apache-2.0",
  "homepage": "https://bruceharrison1984.github.io/Schedulely/",
  "repository": {
    "url": "https://github.com/bruceharrison1984/Schedulely",
    "type": "git"
  },
  "devDependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "dependencies": {
    "@ladle/react": "^2.4.5",
    "@rollup/plugin-inject": "^5.0.3",
    "@swc/core": "^1.3.25",
    "@swc/helpers": "^0.4.14",
    "@swc/jest": "^0.2.24",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/chance": "^1.1.3",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.4",
    "@types/react": "17.0.2",
    "@types/react-test-renderer": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "autoprefixer": "^10.4.12",
    "barrelsby": "^2.5.1",
    "cssnano": "^5.1.13",
    "cssnano-preset-advanced": "^5.3.8",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.2.1",
    "postcss": "^8.4.16",
    "react-test-renderer": "^17.0.2",
    "resize-observer-polyfill": "^1.5.1",
    "rollup": "^3.9.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-ts": "^3.0.2",
    "sass": "^1.55.0"
  }
}
