{
  "name": "schedulely",
  "version": "0.0.31",
  "keywords": [
    "schedule",
    "calendar",
    "react",
    "month"
  ],
  "description": "CSS-grid based React calendar with a focus on extensibility and small package size",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "barrels": "barrelsby -c .barrelsby.json",
    "build": "rm -rf dist && rollup -c",
    "lint": "eslint . --ext .ts --ext .tsx",
    "prettier": "prettier --check './src/**.(ts|tsx|js|scss)'",
    "prepare": "husky install",
    "test": "jest",
    "dev": "ladle serve"
  },
  "files": [
    "dist"
  ],
  "author": "Bruce Harrison",
  "license": "Apache-2.0",
  "repository": {
    "url": "https://github.com/bruceharrison1984/Schedulely",
    "type": "git"
  },
  "devDependencies": {
    "@ladle/react": "^2.4.3",
    "@swc/core": "^1.3.2",
    "@swc/jest": "^0.2.22",
    "@testing-library/react": "12.1.4",
    "@types/chance": "^1.1.3",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@types/react": "^17.0.43",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "autoprefixer": "^10.4.12",
    "barrelsby": "^2.4.0",
    "chance": "^1.1.8",
    "cssnano": "^5.1.13",
    "cssnano-preset-advanced": "^5.3.8",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-ts": "^3.0.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.8.3"
  },
  "peerDependencies": {
    "react": "<=17.0.2",
    "react-dom": "<=17.0.2"
  },
  "dependencies": {
    "@swc/helpers": "^0.4.11",
    "date-fns": "^2.29.2"
  }
}
