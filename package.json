{
  "name": "schedulely",
  "version": "0.0.28",
  "keywords": [
    "schedule",
    "calendar",
    "react",
    "month"
  ],
  "description": "CSS-grid based React calendar with a focus on extensibility and small package size",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "barrels": "barrelsby -c .barrelsby.json",
    "build": "rm -rf dist && rollup -c",
    "lint": "eslint . --ext .ts --ext .tsx",
    "prettier": "prettier --check './src/**.(ts|tsx|js)'",
    "prepare": "husky install",
    "test": "jest",
    "storybook": "start-storybook -p 6006 --no-open",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "files": [
    "dist"
  ],
  "author": "Bruce Harrison",
  "license": "Apache-2.0",
  "repository": {
    "url": "https://github.com/bruceharrison1984/Schedulely",
    "type": "git"
  },
  "devDependencies": {
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/storybook-deployer": "^2.8.11",
    "@swc/core": "^1.2.161",
    "@swc/jest": "^0.2.20",
    "@testing-library/react": "^12.1.4",
    "@types/chance": "^1.1.3",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.22",
    "@types/react": "^17.0.43",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "autoprefixer": "^10.4.4",
    "barrelsby": "^2.3.3",
    "chance": "^1.1.8",
    "cssnano": "^5.1.7",
    "cssnano-preset-advanced": "^5.3.3",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "postcss": "^8.4.12",
    "prettier": "^2.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-ts": "^2.0.5",
    "sass": "^1.50.0",
    "storybook-addon-performance": "^0.16.1",
    "storybook-addon-sass-postcss": "^0.1.0",
    "storybook-addon-swc": "^1.1.4",
    "storybook-dark-mode": "^1.0.9",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "dependencies": {
    "@swc/helpers": "^0.3.8",
    "date-fns": "^2.28.0"
  }
}