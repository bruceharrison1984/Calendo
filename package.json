{
  "name": "schedulely",
  "version": "0.0.15",
  "keywords": [
    "schedule",
    "calendar",
    "react",
    "month"
  ],
  "description": "A React calendar using modern CSS practices",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "barrels": "barrelsby -c .barrelsby.json",
    "build": "rm -rf dist && NODE_ENV=production rollup -c",
    "postbuild": "dts-bundle-generator -o dist/index.d.ts src/index.ts",
    "lint": "eslint . --ext .ts --ext .tsx",
    "prettier": "prettier --check './src/**.(ts|tsx|js)'",
    "prepare": "husky install",
    "test": "jest",
    "storybook": "start-storybook -p 6006 --no-open",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "files": [
    "dist"
  ],
  "author": "Bruce Harrison",
  "license": "Apache-2.0",
  "repository": {
    "url": "https://github.com/bruceharrison1984/Schedulely",
    "type": "git"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-typescript": "^8.3.1",
    "@storybook/addon-actions": "^6.5.0-alpha.49",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-essentials": "^6.5.0-alpha.49",
    "@storybook/addon-links": "^6.5.0-alpha.49",
    "@storybook/builder-webpack5": "^6.5.0-alpha.49",
    "@storybook/manager-webpack5": "^6.5.0-alpha.49",
    "@storybook/react": "^6.5.0-alpha.49",
    "@storybook/storybook-deployer": "^2.8.11",
    "@types/chance": "^1.1.3",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.22",
    "@types/react": "^17.0.41",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "babel-jest": "^27.5.1",
    "barrelsby": "^2.3.3",
    "chance": "^1.1.8",
    "date-fns": "^2.28.0",
    "dts-bundle-generator": "^6.5.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-transform-css": "^3.0.0",
    "prettier": "^2.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "regenerator-runtime": "^0.13.9",
    "rollup": "^2.70.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-import-css": "^3.0.3",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "storybook-addon-performance": "^0.16.1",
    "storybook-dark-mode": "^1.0.10-canary.177.3666.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.2"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  }
}