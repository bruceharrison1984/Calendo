{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "barrels": {
      "dependsOn": [],
      "outputs": [
        "src/**/index.ts"
      ],
      "inputs": [
        "src/**/*.tsx",
        "src/**/*.ts"
      ]
    },
    "test": {
      "dependsOn": [
        "lint",
        "barrels"
      ],
      "outputs": [],
      "inputs": [
        "src/**/*.tsx",
        "src/**/*.ts",
        "__tests__/**/*.tsx",
        "__tests__/**/*.ts"
      ]
    },
    "rollup": {
      "dependsOn": [
        "test"
      ],
      "inputs": [
        "src/**",
        "rollup.config.js"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "build": {
      "dependsOn": [
        "^rollup"
      ],
      "outputs": [
        "build/**"
      ]
    },
    "serve": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "build/**",
        ".docusaurus/**"
      ],
      "cache": false
    },
    "build-ladle": {
      "inputs": [
        "src/**",
        "rollup.config.js"
      ],
      "outputs": [
        "build/**"
      ]
    },
    "lint": {
      "outputs": [],
      "inputs": [
        "./src/**/*.tsx",
        "./src/**/*.ts",
        "./__tests__/**/*.tsx",
        "./__tests__/**/*.ts"
      ]
    },
    "dev": {
      "cache": false
    },
    "dev-docs": {
      "dependsOn": [
        "^rollup"
      ],
      "cache": false
    }
  }
}
